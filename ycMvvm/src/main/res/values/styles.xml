<resources>
    <!-- 对话框风格 -->
    <style name="YcCommonDialogStyle" parent="Theme.MaterialComponents.Light.Dialog">
        <item name="android:windowBackground">@color/yc_transparent</item>
    </style>
    <!-- 加载框框风格 -->
    <style name="YcLoadingDialogStyle" parent="Theme.MaterialComponents.Light.Dialog">
        <item name="android:windowFrame">@null</item><!--边框-->
        <item name="android:windowIsFloating">true</item><!--是否浮现在activity之上-->
        <item name="android:windowIsTranslucent">false</item><!--半透明-->
        <item name="android:windowNoTitle">true</item><!--无标题-->
        <item name="android:windowBackground">@color/yc_transparent</item><!--背景透明-->
    </style>
    <style name="YcLineStyle">
        <item name="android:background">#EFEFEF</item>
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_height">1dp</item>
    </style>
</resources>